<ul class="breadcrumb">
    <li><a ui-sref="index">Home</a> <span class="divider">/</span></li>
    <li><a ui-sref="admin.index">Admin</a> <span class="divider">/</span></li>
    <li><a ui-sref="admin.examples">Examples</a><span class="divider">/</span></li>
    <li class="active">Details of example</li>
</ul>

<div class="page-header">
    <h1>Details of example</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>Name</th><td>{{ item.name }}</td></tr>

        <tr><th>Description</th><td>{{ item.description }}</td></tr>

    </tbody>
</table>

<div class="well">
    <a ui-sref="admin.examplesEdit({id: item.id})" class="btn btn-mini"><i class="icon-edit"></i> Edit</a>
    <a href="" class="btn btn-mini btn-danger" ng-click="delete(item.id, 'examples')">
        <i class="icon-remove icon-white"></i> Delete</a>
    or
    <a ui-sref="admin.examples">Back to index</a>
</div>
