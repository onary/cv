<ul class="breadcrumb">
    <li><a ui-sref="index">Home</a> <span class="divider">/</span></li>
    <li><a ui-sref="admin.index">Admin</a> <span class="divider">/</span></li>
    <li><a ui-sref="admin.sources">Sources</a><span class="divider">/</span></li>
    <li class="active">Details of source</li>
</ul>

<div class="page-header">
    <h1>Details of source</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>Name</th><td>{{ item.name }}</td></tr>
        <tr><th>Description</th><td>{{ item.description }}</td></tr>
        <tr><th>Code</th><td>{{ item.code }}</td></tr>
        <tr><th>example_id</th><td>{{ item.example_id }}</td></tr>
    </tbody>
</table>

<div class="well">
    <a ui-sref="admin.sourcesEdit({id: item.id})" class="btn btn-mini"><i class="icon-edit"></i> Edit</a>
    <a href="" class="btn btn-mini btn-danger" ng-click="delete(item.id, 'source')">
        <i class="icon-remove icon-white"></i> Delete</a>
    or
    <a ui-sref="admin.sources">Back to index</a>
</div>
