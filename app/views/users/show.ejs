<ul class="breadcrumb">
    <li><a ui-sref="index">Home</a> <span class="divider">/</span></li>
    <li><a ui-sref="admin.index">Admin</a> <span class="divider">/</span></li>
    <li><a ui-sref="admin.users">Users</a><span class="divider">/</span></li>
    <li class="active">Details of user</li>
</ul>

<div class="page-header">
    <h1>Details of user</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>Name</th><td>{{ item.name }}</td></tr>

        <tr><th>provider</th><td>{{ item.provider }}</td></tr>

        <tr><th>providerID</th><td>{{ item.providerID }}</td></tr>

        <tr><th>email</th><td>{{ item.email }}</td></tr>

        <tr><th>createdAt</th><td>{{ item.created_at }}</td></tr>

        <tr><th>isadmin</th><td>{{ item.isadmin }}</td></tr>
    </tbody>
</table>

<div class="well">
    <a ui-sref="admin.usersEdit({id: item.id})" class="btn btn-mini"><i class="icon-edit"></i> Edit</a>
    <a href="" class="btn btn-mini btn-danger" ng-click="delete(item.id, 'users')">
        <i class="icon-remove icon-white"></i> Delete</a>
    or
    <a ui-sref="admin.users">Back to index</a>
</div>
